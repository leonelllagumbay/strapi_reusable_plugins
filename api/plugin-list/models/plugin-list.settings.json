{
  "kind": "collectionType",
  "collectionName": "plugin_lists",
  "info": {
    "name": "PluginList"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "preview-content": {
      "previewable": true,
      "url": ":baseUrl/?preview=true&contentType=:contentType&id=:id&name=<%= name %>&slug=<%= description %>",
      "usesValuesInUrl": true
    }
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    }
  }
}
